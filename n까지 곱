//1부터 n까지의 곱 구하기
#include<stdio.h>

int main(void)
{             
	int i, n, num;                    
	printf("정수를 입력하시오 : ");
	scanf("%d", &n);               
  
	i = 1;                 
	num = 1;
	while (i <= n)
	{
		num *= i;                  
		i++;                        
	}
	printf("1부터 %d까지의 곱은 %d입니다.\n", n, num);
	return 0;
}
